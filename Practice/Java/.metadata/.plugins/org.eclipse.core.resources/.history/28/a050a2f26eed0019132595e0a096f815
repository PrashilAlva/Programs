			switch(ch) {
			case 1:insert();
			break;
			case 2:delet();
			break;
			case 3:display();
			break;
			case 4:loop=false;
			break;
			default:System.out.println("Invalid Choice!!!\nTry Again...");
			break;
			}
		}
	}
