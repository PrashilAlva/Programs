
public class AmericanFlag {

	public static void main(String[] args) {
		for(int i=1;i<=15;i++) {
			if(i%2==0) {
				for(int j=1;j<=30;j++) {
					if(j>11) {
						System.out.print("=");
					}
					else {
						System.out.print("* ");
					}
				}
			}
			else {
				for(int j=1;j<=30;j++) {
					if(j>11) {
						System.out.print("=");
					}
					else {
						System.out.print("* ");
					}
				}
			}
			System.out.println();
		}

	}

}
