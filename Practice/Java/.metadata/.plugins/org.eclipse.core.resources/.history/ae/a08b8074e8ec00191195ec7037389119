
public class AmericanFlag {

	public static void main(String[] args) {
		for(int i=1;i<=15;i++) {
			for(int j=1;j<=30;j++) {
			if(i>9) {
				System.out.print("=");
			}
			else {
				if(j<9) {
					System.out.print("*");
				}
				else {
					System.out.print("=");
				}
			}
			}
			System.out.println();

	}
	}
}
